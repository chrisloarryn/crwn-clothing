{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src"
  },
  "paths": {
    "Components/*": [
      "components/*"
    ],
    "Mock/*": [
      "mock/*"
    ],
    "Pages/*": [
      "pages/*"
    ],
    "Types/*": [
      "types/*"
    ],
    "Hooks/*": [
      "hooks/*"
    ],
  },
  "include": [
    "./src"
  ],
}